
<% layout('layout/layout.ejs') -%>
<body>
    <h3><%=list.title %> </h3>
    <ul>
        <li><%=list.title %></li>
        <li><%=list.description %></li>
        <li>&#8377;<%=list.price.toLocaleString('en-IN') %></li>
        <li><%=list.location %></li>
        <li><%=list.country %></li>
        <li><img src="<%=list.image %>" alt=""></li>
        <a href="http://localhost:8080/listing/<%= list.id %>/edit">Edit</a>
    </ul>
</body>
